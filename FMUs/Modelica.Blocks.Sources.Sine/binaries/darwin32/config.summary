# Makefile generated by OpenModelica
CC=i386-apple-darwin15-cc
AR=
CFLAGS=-Os -flto -fno-stack-protector -Wno-parentheses-equality -Wno-unused-variable -fPIC
LD=$(CC) -shared
LDFLAGS=-flto -shared -lm -lpthread  -shared
DLLEXT=.dylib
NEED_RUNTIME=1
NEED_DGESV=1
FMIPLATFORM=darwin32
# Note: Simulation of the fmu with dymola does not work with -finline-small-functions (enabled by most optimization levels)
CPPFLAGS= -fdollars-in-identifiers -DOMC_MINIMAL_RUNTIME=1 -DCMINPACK_NO_DLL=1
OMC_NUM_LINEAR_SYSTEMS=0
OMC_NUM_NONLINEAR_SYSTEMS=0
OMC_NUM_MIXED_SYSTEMS=0
OMC_NDELAY_EXPRESSIONS=0
OMC_NVAR_STRING=0

override CPPFLAGS += -Iinclude/ -Iinclude/fmi2 -I.    -DOMC_MODEL_PREFIX=Modelica_Blocks_Sources_Sine -DOMC_NUM_MIXED_SYSTEMS=0 -DOMC_NUM_LINEAR_SYSTEMS=0 -DOMC_NUM_NONLINEAR_SYSTEMS=0 -DOMC_NDELAY_EXPRESSIONS=0 -DOMC_NVAR_STRING=0

